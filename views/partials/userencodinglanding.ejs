<div class="container">
	<div class="row">
		<div class="col-lg-12">
			<h4 class="page-header insetmeta1"><%= encoding.title %> <span class="insetmeta1"><%= encoding.author %></span> <span class="insetmeta2"><%= moment.utc(encoding.date_posted).utcOffset(-1*(new Date().getTimezoneOffset())).format('MMM Do, YYYY, h:mm:ss a') %></span></h4>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-12 center">
			<div class="Poem-Container">
				
				<form>
					<div class="form-group">
						<label for="selectionone">Encoding</label>
						<textarea class="form-control input-lg" id="item" type="text" rows="16" disabled><%= encoding.body %></textarea>
				</form>

				</div>
			</div>
			<a href="/poem/<%= encoding.poem_id %>"><button type="button" class="btn btn-primary btn-lg">Return to Poem Page</button></a>
